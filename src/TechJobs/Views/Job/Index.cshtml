@model TechJobs.ViewModels.SearchJobsViewModel

@{
// TODO #2.1- declare the ViewModel (outside of this @{ } block)
}


<table class="job-listing">
    @{
// TODO #2.2 - display the job fields, one per row (outside of this @{ } block)
    }
    @foreach (var myjob in Model.Jobs)
    {
        <tr>
            <th>Name</th>
            <td>@myjob.Name</td>
        </tr>

        <tr>
            <th>Employer</th>
            <td>@myjob.Employer</td>
        </tr>

        <tr>
            <th>Location</th>
            <td>@myjob.Location</td>
        </tr>

        <tr>
            <th>CoreCompetency</th>
            <td>@myjob.CoreCompetency</td>
        </tr>

        <tr>
            <td>PositionType</td>
            <td>@myjob.PositionType</td>
        </tr>

    }



</table>
    
        